{
  "name": "express-react-boilerplate",
  "version": "1.0.0",
  "description": "Starter code for a new Express/Reactjs project",
  "main": "index.js",
  "scripts": {
    "start": "cd server && npm run start",
    "start:dev": "concurrently \"cd server && npm run start\" \"cd client && yarn start\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "test": "echo \"Error: no test specified\" && exit 1",
    "seed": "cd server && npm run seed",
    "server": "npm run start:dev --prefix server",
    "client": "npm run start --prefix client",
    "build": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PaulLNH/Express-React-Boilerplate.git"
  },
  "keywords": [],
  "author": "Paul Laird",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PaulLNH/Express-React-Boilerplate/issues"
  },
  "homepage": "https://github.com/PaulLNH/Express-React-Boilerplate#readme",
  "dependencies": {
    "concurrently": "^7.0.0"
  },
  "engines": {
    "node": "16.x"
  }
}
